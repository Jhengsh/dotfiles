[user]
  name = Jhengsh
  email = jhengsh.email@gmail.com

[alias]
  cm = commit
  co = checkout
  ch = checkout
  br = branch
  st = status
  l = log --oneline --graph
  cleanup = "!git branch --merged | grep  -v '\\*\\|master\\|develop' | xargs -n 1 git branch -d"
  fp = "!git fetch --prune"

[core]
	excludesfile = ~/.gitignore_global
	whitespace = cr-at-eol
	editor = vim

[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = true
  pager = true
